<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <AssemblyTitle>CLIENT APP WSMCBL SNAPSHOT</AssemblyTitle>
    <AssemblyVersion>6.6.0</AssemblyVersion>
    <AssemblyDescription>Aplicaci√≥n cliente del proyecto WSMCBL desarrollado en Blazor.</AssemblyDescription>
    <RootNamespace>wsmcbl.src</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BlazorTurnstile" Version="2.1.1" />
    <PackageReference Include="ClosedXML" Version="0.104.2" />
    <PackageReference Include="CurrieTechnologies.Razor.SweetAlert2" Version="5.6.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.11" />
    <PackageReference Include="Microsoft.AspNetCore.Authorization" Version="8.0.11" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="wwwroot\vendor\chart.js\Chart.umd.min.js" />
  </ItemGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="System\Properties\launchSettings.json" />
    <_ContentIncludedByDefault Remove="Homepage\bin\Debug\net8.0\wsmcbl.homepage.deps.json" />
    <_ContentIncludedByDefault Remove="Homepage\bin\Debug\net8.0\wsmcbl.homepage.runtimeconfig.json" />
    <_ContentIncludedByDefault Remove="Homepage\bin\Debug\net8.0\wsmcbl.homepage.staticwebassets.runtime.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\Debug\net8.0\staticwebassets.build.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\Debug\net8.0\staticwebassets.development.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\Debug\net8.0\staticwebassets.pack.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\Debug\net8.0\wsmcbl.homepage.sourcelink.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\project.assets.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\project.packagespec.json" />
    <_ContentIncludedByDefault Remove="Homepage\obj\wsmcbl.homepage.csproj.nuget.dgspec.json" />
    <_ContentIncludedByDefault Remove="Homepage\Properties\launchSettings.json" />
  </ItemGroup>
</Project>
